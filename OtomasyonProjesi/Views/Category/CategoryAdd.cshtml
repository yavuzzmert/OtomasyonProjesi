
@model OtomasyonProjesi.Models.Entities.Category

@{
    ViewBag.Title = "CategoryAdd";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Kategori Ekleme Sayfası</h2>

<br />

<form class="form-group" method="post">
    @Html.LabelFor(x=> x.CategoryName)
    @Html.TextBoxFor(x=> x.CategoryName, new {@class="form-control"})
    <br />
    <button class="btn btn-outline-info">Kaydet</button>
</form>

